
<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form class="" action="/compose" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" style="background-color: #121212; color: #ffffff;" spellcheck="false" type="text" name="postTitle">
        <label>Post</label>
        <textarea class="form-control" style="background-color: #121212;  color: #ffffff;" spellcheck="false" name="postBody" rows="5" cols="30"></textarea>
      </div>
      <button class="btn btn-primary" style="border: 0px;" type="submit" name="button">Publish</button>
    </form>

<%- include("partials/footer"); -%>
